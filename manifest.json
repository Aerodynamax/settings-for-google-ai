{
  "name": "Better AI Overview",
  "description": "TODO",
  "version": "0.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup/index.html",
    "default_icon": {
      "16": "imgs/icon.png",
      "48": "imgs/icon.png",
      "128": "imgs/icon.png"
    }
  },
  "permissions": [
    "storage",
    "activeTab"
  ],

  "content_scripts": [
    {
      "js": ["page/content.js"],
      "matches": [
        "https://google.com/search*",
        "https://www.google.com/search*"
      ],
      "run_at": "document_start",
      "world": "MAIN"
    }
  ]
}